{
  "name": "front",
  "version": "4.1.2",
  "description": "Frontend build with gulp",
  "author": "Luminess",
  "license": "ISC",
  "engines": {
    "node": ">=17.1.0",
    "npm": ">=8.1.2"
  },
  "scripts": {
    "start": "npx gulp"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "Firefox ESR",
    "iOS 13"
  ],
  "config": {
    "context": "html"
  },
  "buildPaths": {
    "src": "./sources/",
    "dist": "./assets/",
    "styles": {
      "i": "./sources/scss/",
      "o": "assets/css/"
    },
    "html": {
      "i": "./sources/html/",
      "o": "./assets/"
    },
    "scripts": {
      "i": "./sources/js/",
      "o": "./assets/js/"
    },
    "images": {
      "i": "./sources/img/",
      "o": "./assets/img/",
      "spritesSVG": "./sources/img/icons/"
    },
    "fonts": {
      "i": "./sources/fonts/",
      "o": "./assets/fonts/"
    },
    "data": {
      "i": "./sources/data/",
      "o": "./data/"
    },
    "root": "./sources/root/"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@ronilaukkarinen/gulp-stylelint": "^14.0.6",
    "@splidejs/splide": "^3.6.12",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "bootstrap": "^5.1.3",
    "browser-sync": "^2.27.9",
    "del": "^6.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-webpack-plugin": "^3.1.1",
    "fancy-log": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-exec": "^5.0.0",
    "gulp-file-include": "^2.3.0",
    "gulp-ignore": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-load-plugins": "^2.0.7",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.1.0",
    "node-sass-glob-importer": "^5.3.2",
    "path": "^0.12.7",
    "postcss": "^8.4.8",
    "postcss-normalize": "^10.0.1",
    "sass": "^1.49.9",
    "stylelint": "^14.5.3",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-stream": "^7.0.0"
  }
}
